<% include layout/header %>
    <div class="content_login">
        <div id="login_bx">
            <div id="passbox" style="padding: 10px;">
                <%if(typeof err != 'undefined' && err != ''){%>
                    <div class="alert alert-danger" style="margin: 0;text-align: center;font-size: 20px;"><%=err%><div style="padding: 10px;text-align: center;font-size: 20px;">Redirecting <i class="fa fa-spinner fa-spin"></i></div></div>
                    <script type="text/javascript">
                    setTimeout(function(){
                        window.location.href= "<%=process.env.SERVER_URL%>";
                    },3000);
                    </script>
                <%}%>

                <%if(typeof succ != 'undefined' && succ != ''){%>
                    <div class="alert alert-success" style="margin: 0;text-align: center;font-size: 20px;"><%=succ%><div style="padding: 10px;text-align: center;font-size: 20px;">Redirecting <i class="fa fa-spinner fa-spin"></i></div></div>
                    <script type="text/javascript">
                    setTimeout(function(){
                        window.location.href= "<%=process.env.SERVER_URL%>/orders/recent";
                    },3000);
                    </script>
                <%}%>
            </div>
        </div>
    </div>
  <% include layout/footer %>