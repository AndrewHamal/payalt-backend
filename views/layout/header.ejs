  <body>
    <div class="row pad20 mar0 flexrowalign responsivenone menu_on_top" style="background: transparent; padding: 0px; position: relative; z-index: 5; width: 100%;">
        <div class="row pad20 mar0 flexrowalign responsivenone" id="headerdiv">
            <div class="col-lg-4 col-md-4 col-sm-3 col-6" style="padding: 0px;">
                <!-- <a href="<%=process.env.SERVER_URL%>"> -->
                <a href="<%=process.env.SERVER_URL%>">
                <img class="logosty" src="<%=process.env.SERVER_URL%>/images/logo.png">
                </a>
            </div>
            <div class="col-lg-8 col-md-8 col-sm-12 col-12 tab_right txtright resp_txtcenter" style="padding: 0px;text-align: right;">
                <%if(typeof sessionval.logged_user_id != 'undefined' && sessionval.logged_user_id != ''){ %>
                <%} else {%>
                    <div class="resp_fright_login resp_padtop10 login_butt_div txtright" style="display: none;">
                        <a class="login_butt scroll_white tab_righta" href="/login">Login</a>
                    </div>
                <%}%>
                <div class="resp_padtop10" style="display: inline-block;">
                    <a href="/business" class="tab_righta scroll_white hedrmnus fntcolapy">Business</a>
                    <!-- <a class="tab_righta scroll_white hedrmnus fntcolapy">Personal</a> -->
                    <a href="/faq" class="tab_righta scroll_white hedrmnus colorwhite">FAQ</a>
                </div>
                <%if(typeof sessionval.logged_user_id != 'undefined' && sessionval.logged_user_id != ''){ %>
                    <div class="fright resp_padtop10" style="display: inline-block;">
                        <a href="/profile" class="tab_righta scroll_white hedrmnus">Profile</a>
                        <a href="/logout" class="tab_righta scroll_white hedrmnus">Logout</a>
                    </div>
                <%} else {%>
                    <div class="resp_fright resp_padtop10" style="display: inline-block;">
                        <!--resp_disp_login -->
                        <a class="login_butt scroll_white tab_righta hedrmnus" href="/login">Login</a>
                        <!--site_button site_buttona  style="color: #fff !important; padding: 6px 20px;" -->
                        <!--<a href="/register" class="tab_righta">Sign up</a>-->
                    </div>
                <%}%>
            </div>
        </div>
    </div>

    <div class="wrapmenu notdisplay xsshow adaptive-menu">
        <a id="close_menu" class="adaptive-menu__close"><span>close</span></a>
        <nav>
            <ul class="nav menu topmenu">
                <li><a href="/business">Business</a></li> 
                <!-- <li><a>Personal</a></li> -->
                <li><a href="/faq">FAQ</a></li>
                <%if(typeof sessionval.logged_user_id != 'undefined' && sessionval.logged_user_id != ''){ %>
                    <li><a href="/profile">Profile</a></li>
                    <li><a href="/logout">Logout</a></li>
                <%} else {%>
                    <li><a href="/login">Login</a></li>
                    <li><a href="/register">Sign up</a></li>
                <%}%>
            </ul>
        </nav>
    </div>
    <div class="resmenuhead" style="background: #464646 !important; text-align: center;">
        <a id="open_menu" class="adaptive-menu__open">
            <span></span>
        </a>
        <a href="<%=process.env.SERVER_URL%>">
            <img class="logosty logosty_resp" src="<%=process.env.SERVER_URL%>/images/logo.png">
        </a>
