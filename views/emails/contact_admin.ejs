<% include layout/header %>
    <tr>
        <td valign="top" class="side title" style="border-collapse: collapse;border: 0;margin: 0;padding: 20px;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;vertical-align: top;background-color: white;border-top: none;">
            <table style="font-weight: normal;border-collapse: collapse;border: 0;margin: 0;padding: 0;font-family: Arial, sans-serif;width:100%;">
                <tr>
                    <td colspan="2" class="head-title" style="border-collapse: collapse;border: 0;margin: 0;padding: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 28px;line-height: 34px;font-weight: bold; text-align: center;">
                        <div class="mktEditable" id="main_title">
                            Hello Admin,
                        </div>
                    </td>
                </tr>
                <tr>
                    <td colspan="2" class="sub-title" style="border-collapse: collapse;border: 0;margin: 0;padding: 0;padding-top:5px;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 18px;line-height: 29px;font-weight: bold;text-align: center;">
                        <div class="mktEditable" id="intro_title">
                            You have a new inquiry from <%=sitename%>
                        </div></td>
                </tr>
                <tr>
                    <td colspan="2" class="top-padding" style="border-collapse: collapse;border: 0;margin: 0;padding: 5px;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;"></td>
                </tr>
                <tr>
                    <td class="grey-block" style="border-collapse: collapse;border: 0;margin: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;background-color: #fff; text-align: right;">
                        <div class="mktEditable" id="cta">
                            <div><strong>Name:</strong></div>
                        </div>
                    </td>
                    <td class="grey-block" style="border-collapse: collapse;border: 0;margin: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;background-color: #fff; text-align: left;padding-left: 20px;">
                         <%=username%>
                    </td>
                </tr>
                <tr>
                    <td class="grey-block" style="border-collapse: collapse;border: 0;margin: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;background-color: #fff; text-align: right;">
                        <div class="mktEditable" id="cta">
                            <div><strong>Email Address</strong>:</div>
                        </div>
                    </td>
                    <td class="grey-block" style="border-collapse: collapse;border: 0;margin: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;background-color: #fff; text-align: left;padding-left: 20px;">
                         <%=emailaddress%>
                    </td>
                </tr>
                <tr>
                    <td class="grey-block" style="border-collapse: collapse;border: 0;margin: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;background-color: #fff; text-align: right;">
                        <div class="mktEditable" id="cta">
			    <div><strong>Subject</strong>:</div>
                        </div>
                    </td>
                    <td class="grey-block" style="border-collapse: collapse;border: 0;margin: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;background-color: #fff; text-align: left;padding-left: 20px;">
                        <%=subject%>
                    </td>
                </tr>
                <tr>
                    <td class="grey-block" style="border-collapse: collapse;border: 0;margin: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;background-color: #fff; text-align: right;">
                        <div class="mktEditable" id="cta">
                            <div><strong>Message</strong>:</div>
                        </div>
                    </td>
                    <td class="grey-block" style="border-collapse: collapse;border: 0;margin: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 26px;background-color: #fff; text-align: left;padding-left: 20px;">
                        <%=message%>
                    </td>
                </tr>
                <tr>
                    <td colspan="2" class="top-padding" style="border-collapse: collapse;border: 0;margin: 0;padding: 15px 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 21px;">
                        <hr size="1" color="#eeeff0">
                    </td>
                </tr>                
                <tr>
                    <td colspan="2" style="border-collapse: collapse;border: 0;margin: 0;padding: 0;-webkit-text-size-adjust: none;color: #555559;font-family: Arial, sans-serif;font-size: 16px;line-height: 24px;">
                        &nbsp;<br>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
<% include layout/footer %>
